{
    "version": 2,
    "builds": [
        { "src": "package.json", "use": "@now/next" }
    ],
    "routes": [{
        "src": "^/service-worker.js$",	
        "dest": "/_next/static/service-worker.js",
        "headers": {
            "cache-control": "public, max-age=43200, immutable",
            "Service-Worker-Allowed": "/"
        }
    }, {
        "src": "/(.+)/(?<articleId>[\\d]+)",
        "dest": "/article?articleId=$articleId"
    }, {
        "src": "/(.+)/(?<articleId>[\\d]+)/+.*$",
        "dest": "/article?articleId=$articleId"
    }],
    "regions": ["bru"]
}
